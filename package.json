{
  "name": "react-icon-library",
  "version": "1.0.0",
  "description": "React Icon library using SVGR",
  "main": "dist/react-icon-library.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "svgr": "npx @svgr/cli --title-prop --replace-attr-values '#303135=currentColor' --out-dir src assets",
